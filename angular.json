{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"login": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/login",
			"sourceRoot": "projects/login/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": "dist/login",
						"index": "projects/login/src/index.html",
						"browser": "projects/login/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/login/tsconfig.app.json",
						"assets": [
							"projects/login/src/favicon.ico",
							"projects/login/src/assets"
						],
						"styles": [
							"node_modules/primeng/resources/themes/lara-light-blue/theme.css",
    					"node_modules/primeng/resources/primeng.min.css",
							"node_modules/primeflex/primeflex.css",
							"projects/login/src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "login:build:production"
						},
						"development": {
							"buildTarget": "login:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "login:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/login/tsconfig.spec.json",
						"assets": [
							"projects/login/src/favicon.ico",
							"projects/login/src/assets"
						],
						"styles": [
							"projects/login/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
    },
